# Ignore node modules
node_modules/

# Ignore build artifacts
dist/
build/
lib/

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore lock files
package-lock.json
pnpm-lock.yaml
yarn.lock

# Ignore logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore generated files
coverage/
public/
